<div id="events" class="ttc-section-wrap">
  <h1 class="ttc-h1">Events</h1>
  <div class="pure-g ttc-events">
    {{#each events}}
      <div class="pure-u-1 pure-u-md-1-2">
        <a href="{{url}}" target="_blank">
          <h2 class="ttc-h2">{{title}}</h2>
          <ul class="fa-ul">
            <li><i class="fa-li fa fa-calendar"></i>{{date}}</li>
            <li><i class="fa-li fa fa-clock-o"></i>{{time}}</li>
            <li><i class="fa-li fa fa-map-marker"></i>{{location}}</li>
          </ul>
        </a>
      </div>
    {{/each}}
  </div>
</div>

